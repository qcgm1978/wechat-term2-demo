.home-spec {
  font-size: 24rpx;
  color: #707070;
  margin-left: 21rpx;
  width: 90%;
  display: inline-block;
}

.opacity-sold-out {
  opacity: 0.3;
}

.store {
  background-image: linear-gradient(90deg, #eb6112 0%, #febb5c 100%);
  justify-content: flex-start;
  padding-left: 16rpx;
}

.icon-wares {
  width: 36rpx;
  height: 36rpx;
}

.icon-txt {
  font-size: 32rpx;
  color: #ee711f;
  margin: 22rpx 10rpx;
}

.title {
  font-size: 30rpx;
  color: rgb(85, 85, 85);
  padding-left: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.flow-container {
  background-color: #fff;
  padding: 30rpx 0;
}

.flow {
  justify-content: center;
}

.flow-line {
  width: 125rpx;
  border-top: 2px rgb(197, 197, 197) dotted;
  height: 1rpx;
  transform: translate(0, 48rpx);
}

.check {
  width: 260rpx;
  text-align: center;
  height: 70rpx;
  background: url('data:image/png;base64,');
  line-height: 70rpx;
  color: #fff;
  background-size: cover;
}

.check-container {
  justify-content: space-around;
  margin-top: 30rpx;
}

.flow text {
  width: 100rpx;
  font-size: 21rpx;
  color: rgb(145, 144, 144);
  text-align: center;
}

.flow image {
  width: 100rpx;
  height: 100rpx;
}

.hot-card {
  position: relative;
  /* margin: 32rpx 52rpx 0; */
  height: 234rpx;
  background: #fff;
  box-shadow: 0 2rpx 13rpx 5rpx rgba(0, 0, 0, 0.02);
  border-radius: 13rpx;
}

.hot-name {
  position: absolute;
  display: flex;
  align-items: center;
  left: 44rpx;
  top: 41rpx;
  width: 300rpx;
}

.hot-name-line {
  width: 24rpx;
  height: 39rpx;
}

.hot-name-text {
  margin: 0 20rpx;
  font-size: 28rpx;
  color: #34373d;
  font-weight: bold;
}

.hot-info {
  position: absolute;
  width: 259rpx;
  height: 188rpx;
  left: 42rpx;
  bottom: 0;
}

.hot-image {
  position: absolute;
  top: 53rpx;
  right: 47rpx;
  width: 241rpx;
  height: 241rpx;
}

.list-title {
  display: flex;
  margin: 15rpx 0;
}

.list-title-line {
  width: 21rpx;
  height: 34rpx;
}

.list-title-text {
  margin: 0 20rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.product-row {
  display: flex;
  justify-content: center;
  margin-bottom: 8rpx;
}

.product-card {
  background: #fff;
  box-shadow: 0 2rpx 13rpx 5rpx rgba(0, 0, 0, 0.02);
  border-radius: 4rpx;
  position: relative;
}

.product-image {
  max-width: 350rpx;
  border-radius: 10rpx;
  height: 350rpx;
}

.product-name {
  min-height:0rpx;
  margin: 0 21rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  height:40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.product-spec {
  margin: 0 21rpx;
  font-size: 24rpx;
  color: #707070;
  line-height: 40rpx;
  height:40rpx;

  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.product-meta {
  display: flex;
  align-items: center;
  margin: 9rpx 21rpx 15rpx;
}

.product-money {
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  color: #34373d;
  font-weight: 900;
  background: #f7e687;
}

.product-money {
  font-size: 24rpx;
}

.product-add {
  width: 40rpx;
  height: 40rpx;
  background: #ec6515;
  color: #fff;
  border-radius: 50%;
}

.minus {
  font-size: 20rpx;
  color: #f2741f;
  letter-spacing: 0;
  text-align: center;
  background-color: #fff6f0;
  border-radius: 8rpx;
  width: 74rpx;
  height: 32rpx;
  line-height: 32rpx;
  margin-left: 5rpx;
}

.product-add text {
  width: 32rpx;
  height: 32rpx;
  line-height: 25rpx;
  text-align: center;
  margin-left:3rpx;

}

.product-price {
  flex: 1;
  /* margin: 0 10rpx; */
  font-size: 32rpx;
  color: #ee711f;
  align-items: center;
  font-weight:bold;
}

.product-add-button {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 80rpx;
  height: 80rpx;
  opacity: 0;
}
.sales-unit{
  font-size: 24rpx;
color: #707070;
letter-spacing: 0;
line-height: 44rpx;
font-weight:normal;
max-width:110rpx;

}