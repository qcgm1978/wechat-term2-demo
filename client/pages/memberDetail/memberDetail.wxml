<view>
  <navigator class='row center' style="width: 100%;background: #fff;margin-top: 30rpx;" url='../transactionList/transactionList'>

    <image src='./img/my-order.png' class='icon' style="margin-left: 55rpx;"></image>
    <view class='column regular' style="align-items: left;">
      <text class='name'>我的订单</text>
      <!-- <text class='name-txt'>{{name}}</text> -->
    </view>
    <view class='center' style="justify-content: flex-end;flex-grow: 1;">
      <image src='./img/arrow.png' class='order-arrow'></image>
    </view>
  </navigator>
  <view class='center-center regular' style="height: 76rpx;font-size:28rpx;color:rgb(51, 51, 51);">
    <view class='short-line'></view>
    <text class='dot'>.</text>店铺信息
    <text class='dot'>.</text>
    <view class='short-line'></view>
  </view>
  <view class='column container-detail'>
    <!-- <view class='column'>
      <image src='{{avatar}}' class='profile center-center'></image>
      <text class='profile-name regular'>{{nickName}}</text>
    </view> -->
    <!-- <view style='width:100%;transform:translate(0,100rpx);'> -->
    <!-- <view class='line'></view> -->
    <view class='row center' style="width: 100%;">
      <image src='./img/shop.png' class='icon'></image>
      <view class='column regular' style="align-items: left;">
        <text class='name'>店铺名称</text>
        <text class='name-txt'>{{name}}</text>
      </view>
    </view>
    <view class='line'></view>
    <view class='row center' style="width: 100%;">
      <image src='./img/address.png' class='icon'></image>
      <view class='column regular' style="align-items: left;">
        <text class='name'>店铺地址</text>
        <text class='name-txt'>{{address}}</text>
      </view>
    </view>
    <view class='line'></view>
    <view class='row center' style="width: 100%;">
      <image src='./img/shopkeeper.png' class='icon'></image>
      <view class='column regular' style="align-items: left;">
        <text class='name'>店主姓名</text>
        <text class='name-txt'>{{profileName}}</text>
      </view>
    </view>
    <view class='line'></view>
    <view class='row center' style="width: 100%;">
      <image src='./img/id.png' class='icon'></image>
      <view class='column regular' style="align-items: left;">
        <text class='name'>门店ID</text>
        <text class='name-txt'>{{id}}</text>
      </view>
    </view>
  </view>
  <view class='exit'>退出登录</view>
  <!-- </view> -->
</view>